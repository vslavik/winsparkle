#: src/ui.cpp:325 src/ui.cpp:674 src/ui.cpp:690
msgid "Software Update"
msgstr "Ažuriranje softvera"

#: src/ui.cpp:513
msgid "Release notes:"
msgstr "Napomene:"

#: src/ui.cpp:539
msgid "Skip this version"
msgstr "Preskoči ovu verziju"

#: src/ui.cpp:545
msgid "Remind me later"
msgstr "Podsjeti me kasnije"

#: src/ui.cpp:550 src/ui.cpp:563
msgid "Install update"
msgstr "Instaliraj nadogradnju"

#: src/ui.cpp:673
#, c-format
msgid "%s cannot be restarted."
msgstr "%s nije moguće ponovo pokrenuti."

#: src/ui.cpp:676
msgid "Make sure that you don't have any unsaved documents and try again."
msgstr "Provjerite da li ste sačuvali sve dokumente i pokušajte ponovo."

#: src/ui.cpp:683
msgid "Launching the installer..."
msgstr "Pokrećem instalaciju..."

#: src/ui.cpp:689
msgid "Failed to launch the installer."
msgstr "Pokretanje instalacije nije uspjelo."

#: src/ui.cpp:732
msgid "Checking for updates..."
msgstr "Provjeravam ima li nadogradnji..."

#: src/ui.cpp:734 src/ui.cpp:815 src/ui.cpp:903
msgid "Cancel"
msgstr "Poništi"

#: src/ui.cpp:760
msgid "You're up to date!"
msgstr "Imate najnoviju verziju!"

#: src/ui.cpp:767
#, c-format
msgid "%s %s is currently the newest version available."
msgstr "%s %s je trenutno najnovija dostupna verzija."

#: src/ui.cpp:780
msgid "Close"
msgstr "Zatvori"

#: src/ui.cpp:801
msgid "Update Error!"
msgstr "Greška pri ažuriranju!"

#: src/ui.cpp:807
msgid "An error occurred in retrieving update information; are you connected to the internet? Please try again later."
msgstr "Desila se greška prilikom dobavljanja informacija o ažuriranju; jeste li povezani na internet? Molimo vas da kasnije pokušate ponovo."

#: src/ui.cpp:810
msgid "The update is improperly signed."
msgstr ""

#: src/ui.cpp:861
#, c-format
msgid "A new version of %s is available!"
msgstr "Dostupna je nova verzija programa %s!"

#: src/ui.cpp:864
msgid "Get update"
msgstr "Preuzmi ažuriranje"

#: src/ui.cpp:870
#, c-format
msgid "%s %s is now available (you have %s). Would you like to download it now?"
msgstr "Dostupan je %s %s (vi imate %s). Želite li odmah izvršiti preuzimanje?"

#: src/ui.cpp:901
msgid "Downloading update..."
msgstr "Preuzimam nadogradnju..."

#. TRANSLATORS: This is the progress of a download, e.g. "3 MB of 12 MB".
#: src/ui.cpp:929
#, c-format
msgid "%s of %s"
msgstr "%s od %s"

#: src/ui.cpp:966
msgid "Ready to install."
msgstr "Spremno za instalaciju."

#: src/ui.cpp:1111
msgid "Check for updates automatically?"
msgstr "Automatska provjera ažuriranja?"

#: src/ui.cpp:1121
#, c-format
msgid "Should %s automatically check for updates? You can always check for updates manually from the menu."
msgstr "Treba li %s automatski provjeravati da li su dostupna ažuriranja? Ovo uvijek možete uraditi ručno iz menija."

#: src/ui.cpp:1135
msgid "Check automatically"
msgstr "Provjeravaj automatski"

#: src/ui.cpp:1140
msgid "Don't check"
msgstr "Ne provjeravaj"

