#: src/ui.cpp:260 src/ui.cpp:586 src/ui.cpp:602
msgid "Software Update"
msgstr "Tarkvara uuendus"

#: src/ui.cpp:437
msgid "Release notes:"
msgstr "Väljalaskemärkmed:"

#: src/ui.cpp:463
msgid "Skip this version"
msgstr "Jäta see versioon vahele"

#: src/ui.cpp:469
msgid "Remind me later"
msgstr "Tuleta mulle hiljem meelde"

#: src/ui.cpp:474 src/ui.cpp:487
msgid "Install update"
msgstr "Paigalda uuendus"

#: src/ui.cpp:585
#, c-format
msgid "%s cannot be restarted."
msgstr "%s ei saa taaskäivitada."

#: src/ui.cpp:588
msgid "Make sure that you don't have any unsaved documents and try again."
msgstr "Veendu, et sul poleks ühtegi salvestamata dokumenti ning proovi seejärel uuesti."

#: src/ui.cpp:595
msgid "Launching the installer..."
msgstr "Paigaldaja käivitamine..."

#: src/ui.cpp:601
msgid "Failed to launch the installer."
msgstr "Paigaldaja käivitamine ebaõnnestus."

#: src/ui.cpp:625
msgid "Checking for updates..."
msgstr "Uuenduste kontrollimine..."

#: src/ui.cpp:627 src/ui.cpp:689 src/ui.cpp:769
msgid "Cancel"
msgstr "Loobu"

#: src/ui.cpp:645
msgid "You're up to date!"
msgstr "Kõik on ajakohane!"

#: src/ui.cpp:652
#, c-format
msgid "%s %s is currently the newest version available."
msgstr "%s %s on hetkel uusim saadaolev versioon."

#: src/ui.cpp:665
msgid "Close"
msgstr "Sulge"

#: src/ui.cpp:684
msgid "Update Error!"
msgstr "Viga uuendamisel!"

#: src/ui.cpp:686
msgid "An error occurred in retrieving update information; are you connected to the internet? Please try again later."
msgstr "Uuenduse info tõmbamisel tekkis tõrge. Kas sul ikka on internetiühendus? Palun proovi hiljem uuesti."

#: src/ui.cpp:727
#, c-format
msgid "A new version of %s is available!"
msgstr "Saadaval on %s uus versioon!"

#: src/ui.cpp:730
msgid "Get update"
msgstr "Hangi uuendus"

#: src/ui.cpp:736
#, c-format
msgid "%s %s is now available (you have %s). Would you like to download it now?"
msgstr "Saadaval on %s uus versioon %s (sul on versioon %s). Kas sa soovid selle kohe alla laadida?"

#: src/ui.cpp:767
msgid "Downloading update..."
msgstr "Uuenduse allalaadimine..."

#. TRANSLATORS: This is the progress of a download, e.g. "3 MB of 12 MB".
#: src/ui.cpp:795
#, c-format
msgid "%s of %s"
msgstr "%s / %s"

#: src/ui.cpp:824
msgid "Ready to install."
msgstr "Paigalduseks valmis."

#: src/ui.cpp:964
msgid "Check for updates automatically?"
msgstr "Kas kontrollida uuendusi automaatselt?"

#: src/ui.cpp:974
#, c-format
msgid "Should %s automatically check for updates? You can always check for updates manually from the menu."
msgstr "Kas %s peaks automaatselt uuendusi otsima? Uuendusi võib alati menüüst käsitsi otsida."

#: src/ui.cpp:988
msgid "Check automatically"
msgstr "Kontrolli automaatselt"

#: src/ui.cpp:993
msgid "Don't check"
msgstr "Ära kontrolli"

