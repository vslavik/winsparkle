#: src/ui.cpp:325 src/ui.cpp:668 src/ui.cpp:684
msgid "Software Update"
msgstr "Softwareopdatering"

#: src/ui.cpp:513
msgid "Release notes:"
msgstr "Udgivelsesnoter:"

#: src/ui.cpp:539
msgid "Skip this version"
msgstr "Spring denne version over"

#: src/ui.cpp:545
msgid "Remind me later"
msgstr "Påmind mig senere"

#: src/ui.cpp:550 src/ui.cpp:563
msgid "Install update"
msgstr "Installer opdateringer"

#: src/ui.cpp:667
#, c-format
msgid "%s cannot be restarted."
msgstr "%s kan ikke genstartes."

#: src/ui.cpp:670
msgid "Make sure that you don't have any unsaved documents and try again."
msgstr "Sørg for at du ikke har nogen dokumenter der ikke er gemt, og prøv så igen."

#: src/ui.cpp:677
msgid "Launching the installer..."
msgstr "Starter installationsprogrammet..."

#: src/ui.cpp:683
msgid "Failed to launch the installer."
msgstr "Kunne ikke starte installationsprogrammet."

#: src/ui.cpp:726
msgid "Checking for updates..."
msgstr "Søger efter opdateringer..."

#: src/ui.cpp:728 src/ui.cpp:809 src/ui.cpp:897
msgid "Cancel"
msgstr "Annuller"

#: src/ui.cpp:754
msgid "You're up to date!"
msgstr "Du er opdateret!"

#: src/ui.cpp:761
#, c-format
msgid "%s %s is currently the newest version available."
msgstr "%s %s er aktuelt den nyeste version der fås."

#: src/ui.cpp:774
msgid "Close"
msgstr "Luk"

#: src/ui.cpp:795
msgid "Update Error!"
msgstr "Opdateringsfejl!"

#: src/ui.cpp:801
msgid "An error occurred in retrieving update information; are you connected to the internet? Please try again later."
msgstr "Der opstod en fejl ved at hente opdateringsoplysninger; er du forbundet til internettet? Prøv venligst igen senere."

#: src/ui.cpp:804
msgid "The update is improperly signed."
msgstr "Opdateringen er ikke underskrevet korrekt."

#: src/ui.cpp:855
#, c-format
msgid "A new version of %s is available!"
msgstr "En ny version af %s er tilgængelig!"

#: src/ui.cpp:858
msgid "Get update"
msgstr "Hent opdatering"

#: src/ui.cpp:864
#, c-format
msgid "%s %s is now available (you have %s). Would you like to download it now?"
msgstr "%s %s er nu tilgængelig (du har %s). Vil du gerne downloade den nu?"

#: src/ui.cpp:895
msgid "Downloading update..."
msgstr "Downloader opdatering..."

#. TRANSLATORS: This is the progress of a download, e.g. "3 MB of 12 MB".
#: src/ui.cpp:923
#, c-format
msgid "%s of %s"
msgstr "%s af %s"

#: src/ui.cpp:960
msgid "Ready to install."
msgstr "Klar til at installere."

#: src/ui.cpp:1105
msgid "Check for updates automatically?"
msgstr "Søg automatisk efter opdateringer?"

#: src/ui.cpp:1115
#, c-format
msgid "Should %s automatically check for updates? You can always check for updates manually from the menu."
msgstr "Skal %s automatisk søge efter opdateringer? Du kan altid søge manuelt efter opdateringer i menuen."

#: src/ui.cpp:1129
msgid "Check automatically"
msgstr "Søg automatisk"

#: src/ui.cpp:1134
msgid "Don't check"
msgstr "Søg ikke"

