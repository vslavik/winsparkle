#: src/ui.cpp:325 src/ui.cpp:668 src/ui.cpp:684
msgid "Software Update"
msgstr "Ažuriranje softvera"

#: src/ui.cpp:513
msgid "Release notes:"
msgstr "Napomene o verziji:"

#: src/ui.cpp:539
msgid "Skip this version"
msgstr "Preskoči ovu verziju"

#: src/ui.cpp:545
msgid "Remind me later"
msgstr "Podseti me kasnije"

#: src/ui.cpp:550 src/ui.cpp:563
msgid "Install update"
msgstr "Instaliraj"

#: src/ui.cpp:667
#, c-format
msgid "%s cannot be restarted."
msgstr "Nije moguće ponovo pokrenuti %s."

#: src/ui.cpp:670
msgid "Make sure that you don't have any unsaved documents and try again."
msgstr "Proverite da li ste sačuvali sve dokumente i pokušajte ponovo."

#: src/ui.cpp:677
msgid "Launching the installer..."
msgstr "Pokretanje instalacije…"

#: src/ui.cpp:683
msgid "Failed to launch the installer."
msgstr "Nije moguće pokrenuti instalaciju."

#: src/ui.cpp:726
msgid "Checking for updates..."
msgstr "Traženje ažuriranja…"

#: src/ui.cpp:728 src/ui.cpp:809 src/ui.cpp:897
msgid "Cancel"
msgstr "Otkaži"

#: src/ui.cpp:754
msgid "You're up to date!"
msgstr "Program je ažuran!"

#: src/ui.cpp:761
#, c-format
msgid "%s %s is currently the newest version available."
msgstr "%s %s je trenutno najnovija verzija."

#: src/ui.cpp:774
msgid "Close"
msgstr "Zatvori"

#: src/ui.cpp:795
msgid "Update Error!"
msgstr "Greška pri ažuriranju"

#: src/ui.cpp:801
msgid "An error occurred in retrieving update information; are you connected to the internet? Please try again later."
msgstr "Došlo je do greške pri preuzimanju informacija o ažuriranju. Proverite da li ste povezani na internet i pokušajte ponovo."

#: src/ui.cpp:804
msgid "The update is improperly signed."
msgstr "Ažuriranje nema važeći digitalni potpis."

#: src/ui.cpp:855
#, c-format
msgid "A new version of %s is available!"
msgstr "Dostupna je nova verzija programa %s!"

#: src/ui.cpp:858
msgid "Get update"
msgstr "Preuzmi"

#: src/ui.cpp:864
#, c-format
msgid "%s %s is now available (you have %s). Would you like to download it now?"
msgstr "Dostupan je %s %s (vi imate verziju %s). Želite li da preuzmete ažuriranje?"

#: src/ui.cpp:895
msgid "Downloading update..."
msgstr "Preuzimanje ažuriranja…"

#. TRANSLATORS: This is the progress of a download, e.g. "3 MB of 12 MB".
#: src/ui.cpp:923
#, c-format
msgid "%s of %s"
msgstr "%s od %s"

#: src/ui.cpp:960
msgid "Ready to install."
msgstr "Spremno za instalaciju."

#: src/ui.cpp:1105
msgid "Check for updates automatically?"
msgstr "Tražiti ažuriranja automatski?"

#: src/ui.cpp:1115
#, c-format
msgid "Should %s automatically check for updates? You can always check for updates manually from the menu."
msgstr "Želite li da %s automatski traži ažuriranja? To možete da uradite i ručno iz menija."

#: src/ui.cpp:1129
msgid "Check automatically"
msgstr "Traži automatski"

#: src/ui.cpp:1134
msgid "Don't check"
msgstr "Ne traži"

