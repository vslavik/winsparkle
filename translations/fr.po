#: src/ui.cpp:260 src/ui.cpp:586 src/ui.cpp:602
msgid "Software Update"
msgstr "Mise à jour du Logiciel"

#: src/ui.cpp:437
msgid "Release notes:"
msgstr "Notes de version :"

#: src/ui.cpp:463
msgid "Skip this version"
msgstr "Ignorer cette version"

#: src/ui.cpp:469
msgid "Remind me later"
msgstr "Me le rappeler ultérieurement"

#: src/ui.cpp:474 src/ui.cpp:487
msgid "Install update"
msgstr "Installer la mise à jour"

#: src/ui.cpp:585
#, c-format
msgid "%s cannot be restarted."
msgstr "%s ne peut être redémarré."

#: src/ui.cpp:588
msgid "Make sure that you don't have any unsaved documents and try again."
msgstr "Assurez-vous que vos documents soient enregistrés et réessayez."

#: src/ui.cpp:595
msgid "Launching the installer..."
msgstr "Démarrage du programme d'installation ... "

#: src/ui.cpp:601
msgid "Failed to launch the installer."
msgstr "Impossible de démarrer le programme d'installation."

#: src/ui.cpp:625
msgid "Checking for updates..."
msgstr "Recherche de mises à jour..."

#: src/ui.cpp:627 src/ui.cpp:689 src/ui.cpp:769
msgid "Cancel"
msgstr "Annuler"

#: src/ui.cpp:645
msgid "You're up to date!"
msgstr "Le programme est à jour !"

#: src/ui.cpp:652
#, c-format
msgid "%s %s is currently the newest version available."
msgstr "%s %s est actuellement la version la plus récente disponible."

#: src/ui.cpp:665
msgid "Close"
msgstr "Fermer"

#: src/ui.cpp:684
msgid "Update Error!"
msgstr "Erreur lors de la mise à jour !"

#: src/ui.cpp:686
msgid "An error occurred in retrieving update information; are you connected to the internet? Please try again later."
msgstr "Une erreur est survenue lors de la récupération des données de mise à jour. Êtes-vous connecté à internet ? Veuillez réessayer ultérieurement."

#: src/ui.cpp:727
#, c-format
msgid "A new version of %s is available!"
msgstr "Une nouvelle version de %s est disponible !"

#: src/ui.cpp:730
msgid "Get update"
msgstr "Mettre à jour"

#: src/ui.cpp:736
#, c-format
msgid "%s %s is now available (you have %s). Would you like to download it now?"
msgstr "%s %s est disponible (vous possédez actuellement %s). Souhaitez-vous la télécharger maintenant ?"

#: src/ui.cpp:767
msgid "Downloading update..."
msgstr "Téléchargement de la mise à jour..."

#. TRANSLATORS: This is the progress of a download, e.g. "3 MB of 12 MB".
#: src/ui.cpp:795
#, c-format
msgid "%s of %s"
msgstr "%s de %s"

#: src/ui.cpp:824
msgid "Ready to install."
msgstr "Prête à être installée."

#: src/ui.cpp:964
msgid "Check for updates automatically?"
msgstr "Vérification automatique des mises à jour ?"

#: src/ui.cpp:974
#, c-format
msgid "Should %s automatically check for updates? You can always check for updates manually from the menu."
msgstr "Souhaitez-vous que %s recherche automatiquement les mises à jours ? Vous pouvez le faire manuellement à partir du menu."

#: src/ui.cpp:988
msgid "Check automatically"
msgstr "Vérifier automatiquement"

#: src/ui.cpp:993
msgid "Don't check"
msgstr "Ne pas vérifier"

