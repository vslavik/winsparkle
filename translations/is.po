#: src/ui.cpp:260 src/ui.cpp:586 src/ui.cpp:602
msgid "Software Update"
msgstr "Hugbúnaðaruppfærsla"

#: src/ui.cpp:437
msgid "Release notes:"
msgstr "Útgáfuupplýsingar:"

#: src/ui.cpp:463
msgid "Skip this version"
msgstr "Sleppa þessari útgáfu"

#: src/ui.cpp:469
msgid "Remind me later"
msgstr "Minna mig á síðar"

#: src/ui.cpp:474 src/ui.cpp:487
msgid "Install update"
msgstr "Setja inn uppfærslu"

#: src/ui.cpp:585
#, c-format
msgid "%s cannot be restarted."
msgstr "Ekki er hægt að endurræsa %s."

#: src/ui.cpp:588
msgid "Make sure that you don't have any unsaved documents and try again."
msgstr "Gakktu úr skugga um að þú sért ekki með nein óvistuð skjöl og reyndu svo aftur."

#: src/ui.cpp:595
msgid "Launching the installer..."
msgstr "Keyri uppsetningarforritið..."

#: src/ui.cpp:601
msgid "Failed to launch the installer."
msgstr "Gat ekki gangsett uppsetningarforritið."

#: src/ui.cpp:625
msgid "Checking for updates..."
msgstr "Athuga með uppfærslur..."

#: src/ui.cpp:627 src/ui.cpp:689 src/ui.cpp:769
msgid "Cancel"
msgstr "Hætta við"

#: src/ui.cpp:645
msgid "You're up to date!"
msgstr "Kerfið er uppfært!"

#: src/ui.cpp:652
#, c-format
msgid "%s %s is currently the newest version available."
msgstr "%s %s er nýjasta útgáfan sem tiltæk er í augnablikinu."

#: src/ui.cpp:665
msgid "Close"
msgstr "Loka"

#: src/ui.cpp:684
msgid "Update Error!"
msgstr "Villa í uppfærslu!"

#: src/ui.cpp:686
msgid "An error occurred in retrieving update information; are you connected to the internet? Please try again later."
msgstr "Villa kom upp við að ná í uppfærsluupplýsingar; ertu tengdur við internetið? Reyndu aftur síðar."

#: src/ui.cpp:727
#, c-format
msgid "A new version of %s is available!"
msgstr "Ný útgáfa af %s er tiltæk!"

#: src/ui.cpp:730
msgid "Get update"
msgstr "Ná í uppfærslu"

#: src/ui.cpp:736
#, c-format
msgid "%s %s is now available (you have %s). Would you like to download it now?"
msgstr "%s %s er núna tiltækt (þú ert með %s). Viltu sækja það núna?"

#: src/ui.cpp:767
msgid "Downloading update..."
msgstr "Sæki uppfærslu..."

#. TRANSLATORS: This is the progress of a download, e.g. "3 MB of 12 MB".
#: src/ui.cpp:795
#, c-format
msgid "%s of %s"
msgstr "%s af %s"

#: src/ui.cpp:824
msgid "Ready to install."
msgstr "Tilbúið fyrir uppsetningu."

#: src/ui.cpp:964
msgid "Check for updates automatically?"
msgstr "Leita sjálfkrafa að uppfærslum?"

#: src/ui.cpp:974
#, c-format
msgid "Should %s automatically check for updates? You can always check for updates manually from the menu."
msgstr "Ætti %s að athuga sjálfkrafa með uppfærslur? Þú getur alltaf athugað handvirkt með uppfærslur úr valmyndvalmyndinni."

#: src/ui.cpp:988
msgid "Check automatically"
msgstr "Leita sjálfvirkt"

#: src/ui.cpp:993
msgid "Don't check"
msgstr "Ekki athuga"

