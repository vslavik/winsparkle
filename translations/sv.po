#: src/ui.cpp:260 src/ui.cpp:586 src/ui.cpp:602
msgid "Software Update"
msgstr "Programuppdatering"

#: src/ui.cpp:437
msgid "Release notes:"
msgstr "Ändringslogg:"

#: src/ui.cpp:463
msgid "Skip this version"
msgstr "Hoppa över denna version"

#: src/ui.cpp:469
msgid "Remind me later"
msgstr "Påminn mig senare"

#: src/ui.cpp:474 src/ui.cpp:487
msgid "Install update"
msgstr "Installera uppdatering"

#: src/ui.cpp:585
#, c-format
msgid "%s cannot be restarted."
msgstr "%s kan inte startas om."

#: src/ui.cpp:588
msgid "Make sure that you don't have any unsaved documents and try again."
msgstr "Se till att du inte har några osparade dokument och försök igen."

#: src/ui.cpp:595
msgid "Launching the installer..."
msgstr "Startar installationsprogrammet..."

#: src/ui.cpp:601
msgid "Failed to launch the installer."
msgstr "Det gick inte att starta installationsprogrammet."

#: src/ui.cpp:625
msgid "Checking for updates..."
msgstr "Söker efter uppdateringar..."

#: src/ui.cpp:627 src/ui.cpp:689 src/ui.cpp:769
msgid "Cancel"
msgstr "Avbryt"

#: src/ui.cpp:645
msgid "You're up to date!"
msgstr "Du har senaste versionen!"

#: src/ui.cpp:652
#, c-format
msgid "%s %s is currently the newest version available."
msgstr "%s %s är för närvarande den senaste tillgängliga versionen."

#: src/ui.cpp:665
msgid "Close"
msgstr "Stäng"

#: src/ui.cpp:684
msgid "Update Error!"
msgstr "Uppdateringsfel!"

#: src/ui.cpp:686
msgid "An error occurred in retrieving update information; are you connected to the internet? Please try again later."
msgstr "Ett fel uppstod när information om uppdateringen skulle hämtas. Kontrollera att du är ansluten till internet och försök igen."

#: src/ui.cpp:727
#, c-format
msgid "A new version of %s is available!"
msgstr "En ny version av %s finns tillgänglig!"

#: src/ui.cpp:730
msgid "Get update"
msgstr "Hämta uppdatering"

#: src/ui.cpp:736
#, c-format
msgid "%s %s is now available (you have %s). Would you like to download it now?"
msgstr "%s %s finns nu tillgänglig (du har %s). Vill du hämta den nu?"

#: src/ui.cpp:767
msgid "Downloading update..."
msgstr "Hämtar uppdatering..."

#. TRANSLATORS: This is the progress of a download, e.g. "3 MB of 12 MB".
#: src/ui.cpp:795
#, c-format
msgid "%s of %s"
msgstr "%s av %s"

#: src/ui.cpp:824
msgid "Ready to install."
msgstr "Klar att installera."

#: src/ui.cpp:964
msgid "Check for updates automatically?"
msgstr "Söka efter uppdateringar automatiskt?"

#: src/ui.cpp:974
#, c-format
msgid "Should %s automatically check for updates? You can always check for updates manually from the menu."
msgstr "Ska %s söka efter uppdateringar automatiskt? Du kan alltid söka efter uppdateringar manuellt från menyn."

#: src/ui.cpp:988
msgid "Check automatically"
msgstr "Kontrollera automatiskt"

#: src/ui.cpp:993
msgid "Don't check"
msgstr "Kontrollera inte"

