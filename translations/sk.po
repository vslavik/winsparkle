#: src/ui.cpp:260 src/ui.cpp:586 src/ui.cpp:602
msgid "Software Update"
msgstr "Aktualizácia programu"

#: src/ui.cpp:437
msgid "Release notes:"
msgstr "Poznámky k verzii:"

#: src/ui.cpp:463
msgid "Skip this version"
msgstr "Preskočiť túto verziu"

#: src/ui.cpp:469
msgid "Remind me later"
msgstr "Pripomenúť neskôr"

#: src/ui.cpp:474 src/ui.cpp:487
msgid "Install update"
msgstr "Inštalovať aktualizáciu"

#: src/ui.cpp:585
#, c-format
msgid "%s cannot be restarted."
msgstr "%s nie je možné reštartovať."

#: src/ui.cpp:588
msgid "Make sure that you don't have any unsaved documents and try again."
msgstr "Uistite sa, že nemáte žiadne neuložené dokumenty a skúste to znova."

#: src/ui.cpp:595
msgid "Launching the installer..."
msgstr "Spúšťanie inštalátora..."

#: src/ui.cpp:601
msgid "Failed to launch the installer."
msgstr "Nepodarilo sa spustiť inštalátor."

#: src/ui.cpp:625
msgid "Checking for updates..."
msgstr "Kontrolujú sa aktualizácie..."

#: src/ui.cpp:627 src/ui.cpp:689 src/ui.cpp:769
msgid "Cancel"
msgstr "Zrušiť"

#: src/ui.cpp:645
msgid "You're up to date!"
msgstr "Program je aktuálny!"

#: src/ui.cpp:652
#, c-format
msgid "%s %s is currently the newest version available."
msgstr "%s %s je aktuálne najnovšia dostupná verzia."

#: src/ui.cpp:665
msgid "Close"
msgstr "Zatvoriť"

#: src/ui.cpp:684
msgid "Update Error!"
msgstr "Chyba aktualizácie!"

#: src/ui.cpp:686
msgid "An error occurred in retrieving update information; are you connected to the internet? Please try again later."
msgstr "Pri získavaní informácií o aktualizácii nastala chyba. Je váš počítač pripojený k internetu? Skúste znova neskôr."

#: src/ui.cpp:727
#, c-format
msgid "A new version of %s is available!"
msgstr "Je dostupná nová verzia programu %s!"

#: src/ui.cpp:730
msgid "Get update"
msgstr "Stiahnuť aktualizáciu"

#: src/ui.cpp:736
#, c-format
msgid "%s %s is now available (you have %s). Would you like to download it now?"
msgstr "Je k dispozícii %s %s (máte verziu %s). Chcete ju stiahnuť teraz?"

#: src/ui.cpp:767
msgid "Downloading update..."
msgstr "Sťahovanie aktualizácie..."

#. TRANSLATORS: This is the progress of a download, e.g. "3 MB of 12 MB".
#: src/ui.cpp:795
#, c-format
msgid "%s of %s"
msgstr "%s z %s"

#: src/ui.cpp:824
msgid "Ready to install."
msgstr "Pripravené na inštaláciu."

#: src/ui.cpp:964
msgid "Check for updates automatically?"
msgstr "Automaticky kontrolovať dostupnosť aktualizácií?"

#: src/ui.cpp:974
#, c-format
msgid "Should %s automatically check for updates? You can always check for updates manually from the menu."
msgstr "Chcete, aby program %s automaticky kontroloval dostupnosť aktualizácií? Aktualizácie môžete kedykoľvek vyhľadať ručne z príslušného menu programu."

#: src/ui.cpp:988
msgid "Check automatically"
msgstr "Kontrolovať automaticky"

#: src/ui.cpp:993
msgid "Don't check"
msgstr "Nekontrolovať"

