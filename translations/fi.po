#: src/ui.cpp:325 src/ui.cpp:668 src/ui.cpp:684
msgid "Software Update"
msgstr "Ohjelmistopäivitys"

#: src/ui.cpp:513
msgid "Release notes:"
msgstr "Julkaisutiedot:"

#: src/ui.cpp:539
msgid "Skip this version"
msgstr "Ohita tämä versio"

#: src/ui.cpp:545
msgid "Remind me later"
msgstr "Muistuta myöhemmin"

#: src/ui.cpp:550 src/ui.cpp:563
msgid "Install update"
msgstr "Asenna päivitys"

#: src/ui.cpp:667
#, c-format
msgid "%s cannot be restarted."
msgstr "%s ei voida käynnistää uudelleen."

#: src/ui.cpp:670
msgid "Make sure that you don't have any unsaved documents and try again."
msgstr "Varmista että olet tallentanut kaikki avoimet tiedostot ja yritä uudelleen."

#: src/ui.cpp:677
msgid "Launching the installer..."
msgstr "Käynnistetään asennusta..."

#: src/ui.cpp:683
msgid "Failed to launch the installer."
msgstr "Asennuksen käynnistys epäonnistui."

#: src/ui.cpp:726
msgid "Checking for updates..."
msgstr "Tarkistetaan päivityksiä..."

#: src/ui.cpp:728 src/ui.cpp:809 src/ui.cpp:897
msgid "Cancel"
msgstr "Peruuta"

#: src/ui.cpp:754
msgid "You're up to date!"
msgstr "Sovellus on ajan tasalla!"

#: src/ui.cpp:761
#, c-format
msgid "%s %s is currently the newest version available."
msgstr "%s %s on uusin saatavilla oleva versio."

#: src/ui.cpp:774
msgid "Close"
msgstr "Sulje"

#: src/ui.cpp:795
msgid "Update Error!"
msgstr "Päivitysvirhe!"

#: src/ui.cpp:801
msgid "An error occurred in retrieving update information; are you connected to the internet? Please try again later."
msgstr "Virhe noudettaessa päivitystietoja. Oletko yhteydessä Internetiin? Yritä myöhemmin uudelleen."

#: src/ui.cpp:804
msgid "The update is improperly signed."
msgstr "Päivityksen allekirjoitus ei kelpaa."

#: src/ui.cpp:855
#, c-format
msgid "A new version of %s is available!"
msgstr "Uusi %s -versio on saatavilla!"

#: src/ui.cpp:858
msgid "Get update"
msgstr "Hanki päivitys"

#: src/ui.cpp:864
#, c-format
msgid "%s %s is now available (you have %s). Would you like to download it now?"
msgstr "%s %s on saatavilla (sinulla on %s). Haluatko ladata sen nyt?"

#: src/ui.cpp:895
msgid "Downloading update..."
msgstr "Ladataan päivitystä..."

#. TRANSLATORS: This is the progress of a download, e.g. "3 MB of 12 MB".
#: src/ui.cpp:923
#, c-format
msgid "%s of %s"
msgstr "%s/%s"

#: src/ui.cpp:960
msgid "Ready to install."
msgstr "Valmis asennettavaksi."

#: src/ui.cpp:1105
msgid "Check for updates automatically?"
msgstr "Tarkistetaanko päivitykset automaattisesti?"

#: src/ui.cpp:1115
#, c-format
msgid "Should %s automatically check for updates? You can always check for updates manually from the menu."
msgstr "Pitäisikö %s -päivitykset tarkistaa automaattisesti? Voit aina tarkistaa päivitykset myös manuaalisesti valikosta."

#: src/ui.cpp:1129
msgid "Check automatically"
msgstr "Tarkista automaattisesti"

#: src/ui.cpp:1134
msgid "Don't check"
msgstr "Älä tarkista"

